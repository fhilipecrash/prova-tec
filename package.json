{
  "name": "prova-tec",
  "version": "1.0.0",
  "description": "Implementação de TDD para a matéria de Tópicos Especiais",
  "main": "src/main.ts",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register ./src/main.ts --files",
    "start": "node ./build/src/index.js",
    "build": "rm -rf ./build && tsc && tsc-alias",
    "lint": "eslint .",
    "lint:clear": "rm -f .eslintcache",
    "lint:fix": "npm run lint --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm run test --watch",
    "test:staged": "lint-staged",
    "test:coverage": "npm run test --config jest.coverage.config.ts --coverage"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16"
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "eslint-config-love": "^113",
    "eslint-config-prettier": "^9",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^28",
    "eslint-plugin-markdown": "^5",
    "eslint-plugin-n": "^17",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^7",
    "jest": "^29",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^15",
    "prettier": "^3",
    "ts-jest": "^29",
    "ts-node": "^10",
    "ts-node-dev": "^2",
    "tsc-alias": "^1",
    "tsconfig-paths": "^4",
    "typescript": "^5",
    "typescript-transform-paths": "^3"
  }
}
